  "Xperience Admin Module": {
    "prefix": ["xpc-admin-module"],
    "description": "An Xperience by Kentico custom Admin Module (C#)",
    "isFileTemplate": true,
    "body": [
      "using CMS;",
      "using Kentico.Xperience.Admin.Base;",
      "using ${1:MySite.Admin};",
      "",
      "[assembly: RegisterModule(typeof(${TM_FILENAME_BASE}))]",
      "",
      "[assembly: UICategory(",
      "    codeName: ${TM_FILENAME_BASE}.CATEGORY,",
      "    name: \"${TM_FILENAME_BASE/([a-zA-Z])(?=[A-Z])/${1} /g}\",",
      "    icon: Icons.CustomElement,",
      "    order: 100)]",
      "",
      "namespace ${1:MySite.Admin};",
      "",
      "internal class ${TM_FILENAME_BASE} : AdminModule",
      "{",
      "    public const string CATEGORY = \"${1/[\\.]/-/g}.${TM_FILENAME_BASE/([a-zA-Z])(?=[A-Z])/${1}-/g}\";",
      "",
      "    public ${TM_FILENAME_BASE}() : base(\"${1}.${TM_FILENAME_BASE/([a-zA-Z])(?=[A-Z])/${1}-/g}\") { }",
      "",
      "    protected override void OnInit()",
      "    {",
      "        base.OnInit();",
      "",
      "        RegisterClientModule(\"${1/[\\.]/-/g}\", \"${TM_FILENAME_BASE/([a-zA-Z])(?=[A-Z])/${1}-/g}\");${0}",
      "    }",
      "}"
    ]
  },
